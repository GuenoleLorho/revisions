<!DOCTYPE html>
<html>

<head>
  <title>Index</title>
  <% include ../partials/head %>
</head>

<body class="container">
  <header>
    <% include ../partials/header %>
  </header>

  <main>
    <div class="jumbotron">

      <h2>Quotes!</h2>
      <h1><%= output %></h1>
      <h2>Loop</h2>
      <ul>
        <% quotesarray.forEach(function(quote1){%>
          <li>
            <%= quote1.name %> -
            <%= quote1.quote %>
          </li>
        <% }); %>
      </ul>


    </div>
  </main>


  <footer>
    <% include ../partials/footer %>
  </footer>

</body>

</html>
